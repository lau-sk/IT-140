word = input()
password = ''

for c in word:
    if (c =='a'):
        password += "@"
    elif (c == 'i'):
        password += "!"
    elif (c == 'm'):
        password += "M"
    elif (c == 'o'):
        password += "."
    elif (c == 'B'):
        password += "8"
    else:
        password += c

password += "q*s"
print(password)
