user_num = int(input('Enter integer:\n'))

# Type your code here
print("You entered: " + str(user_num))
num_squared = user_num * user_num
print(str(user_num) + " squared is " + str(num_squared))
num_cubed = user_num * user_num * user_num
print("And " + str(user_num) + " cubed is " + str(num_cubed) + " !!")

user_num2 = int(input('Enter another integer:\n'))
sum = user_num + user_num2
print(str(user_num) + " + " + str(user_num2) + " is " + str(sum))
product = user_num * user_num2
print(str(user_num) + " * " + str(user_num2) + " is " + str(product))

